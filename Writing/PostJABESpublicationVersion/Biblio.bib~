@article{Four82a,
    OPTabstract = {{We present a general theory for analyzing catch at age data for a fishery. This theory seems to be the first to address itself properly to the stochastic nature of the errors in the observed catch at age data. The model developed is very flexible and accommodates itself easily to the inclusion of extra information such as fishing effort data or information about errors in the aging procedure. An example is given to illustrate the use of the model.Key words: cohort analysis, virtual population analysis, maximum likelihood estimation, aging errors}},
    author = {Fournier, D. and Archibald, C.P.},
