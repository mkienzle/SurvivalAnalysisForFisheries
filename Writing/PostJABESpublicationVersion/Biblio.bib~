@article{Lester2009129,
title = "Movement of sea mullet {\it \uppercase{M}ugil cephalus} as indicated by a parasite ",
journal = FishRes,
volume = "96",
number = "2–3",
pages = "129 - 132",
year = "2009",
OPTnote = "",
OPTissn = "0165-7836",
OPTdoi = "http://dx.doi.org/10.1016/j.fishres.2008.10.006",
OPTurl = "http://www.sciencedirect.com/science/article/pii/S0165783608003330",
author = "R.J.G. Lester and S.E. Rawlinson and L.C. Weaver",
OPTkeywords = "Mugil",
OPTkeywords = "Mullet",
OPTkeywords = "Plethorchis",
OPTkeywords = "Parasite tag",
OPTkeywords = "Migration",
OPTkeywords = "Sanguinicolid",
OPTkeywords = "Blood fluke",
OPTkeywords = "Australia ",
OPTabstract = "Most sea mullet in \{SE\} Queensland have numerous brown patches over the surface of the intestine. These are the remains of eggs of the blood fluke Plethorchis acanthus. Adult parasites are rarely found and when they occur are not within blood vessels but on the surface of the viscera over which they crawl as they lay their eggs. Infections apparently originate when fish are juveniles in fresh water. The abundance of the parasite in \{SE\} Queensland and the absence of the parasite from mullet further south indicates that the wave of fish that appears to move up the coast in the autumn may be an illusion. The parasite data suggest that large numbers of fish are not moving from northern \{NSW\} into \{SE\} Queensland. It is more likely that the apparent seasonal wave is caused by fish sequentially leaving local estuaries for the sea as the temperature drops in autumn. "
}

