\section*{Figures}

%%%%% LTMP length at age data and von Bertalanffy growth function
    \begin{figure}[h!]
      %\begin{center}
        \includegraphics[scale=0.6, angle = -90]{../Results/Graphics/VBGF-usingLTMPdata.ps}
        \caption{LTMP mean length ($\pm$ 1 sd) at age data: black dots and lines represent estimates with less than 5 observations; in blue, those belonging to age-groups lower or equal to 30 months; in red, those included in all analysis. The overlayed line represent results of the $\chi^2$ fit of the von Bertalanffy growht function: in red, using only the data points in red; in blue, using both the blue and the red data points.}
        \label{fig:VBGF-usingLTMPdata}
      %\end{center}
   \end{figure}

