
# Give a number to each cohort

# ARGUMENTS n: number of rows of the data matrix
#           p: number of columns of the data matrix
number.cohorts <- function(n=5, p=5){

    mat <- matrix(NA, nrow = n, ncol = p)
    for(i in 1:n){
        for(j in 1:p){
            mat[i,j] <- i - j + p
        }
    }
    
    return(mat)        
}
